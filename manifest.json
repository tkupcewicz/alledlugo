{
  "manifest_version": 2,
 
  "name": "WorkTime",
  "version": "0.1.0",
  "description": "Change prices to time of work",
 
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://allegro.pl/*"]
  }],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}